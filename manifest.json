{
  "manifest_version": 2,
  "name": "HashBox",
  "version": "1.0",
  "description": "Allows a user to hash text and later files inside of your web browser.",
  "icons": {
    "48": "icons/border-48.png"
  },
  "browser_action": {
    "default_icon": "icons/HashSymbol.svg",
    "default_title": "HashBox"
  },
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"]
  }
}